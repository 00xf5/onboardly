{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" },
    { "src": "api/**/*.ts", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "(.*)", "dest": "/index.html" }
  ],
  "functions": {
    "api/**.ts": {
      "memory": 512,
      "maxDuration": 10
    }
  },
  "crons": []
}
